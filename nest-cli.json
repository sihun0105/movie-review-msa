{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "./",
  "entryFile": "src/main",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/api-gateway",
  "projects": {
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "src/main",
      "sourceRoot": "./",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json",
        "assets": [
          "proto/*.proto"
        ],
        "watchAssets": true
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "src/main",
      "sourceRoot": "./",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json",
        "assets": [
          "proto/*.proto"
        ],
        "watchAssets": true
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "src/main",
      "sourceRoot": "./",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json",
        "assets": [
          "proto/*.proto"
        ],
        "watchAssets": true
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "prisma": {
      "type": "library",
      "root": "libs/prisma",
      "entryFile": "index",
      "sourceRoot": "libs/prisma/src",
      "compilerOptions": {
        "tsConfigPath": "libs/prisma/tsconfig.lib.json"
      }
    },
    "utils": {
      "type": "library",
      "root": "libs/utils",
      "entryFile": "index",
      "sourceRoot": "libs/utils/src",
      "compilerOptions": {
        "tsConfigPath": "libs/utils/tsconfig.lib.json"
      }
    },
    "reply": {
      "type": "application",
      "root": "apps/reply",
      "entryFile": "src/main",
      "sourceRoot": "./",
      "compilerOptions": {
        "tsConfigPath": "apps/reply/tsconfig.app.json",
        "assets": [
          "proto/*.proto"
        ],
        "watchAssets": true
      }
    },
    "cron": {
      "type": "application",
      "root": "apps/cron",
      "entryFile": "src/main",
      "sourceRoot": "./",
      "compilerOptions": {
        "tsConfigPath": "apps/cron/tsconfig.app.json",
        "assets": [
          "proto/*.proto"
        ],
        "watchAssets": true
      }
    },
    "movie": {
      "type": "application",
      "root": "apps/movie",
      "entryFile": "src/main",
      "sourceRoot": "./",
      "compilerOptions": {
        "tsConfigPath": "apps/movie/tsconfig.app.json",
        "assets": [
          "proto/*.proto"
        ],
        "watchAssets": true
      }
    },
    "crawling": {
      "type": "library",
      "root": "libs/crawling",
      "entryFile": "index",
      "sourceRoot": "libs/crawling/src",
      "compilerOptions": {
        "tsConfigPath": "libs/crawling/tsconfig.lib.json"
      }
    },
    "embedding": {
      "type": "application",
      "root": "apps/embedding",
      "entryFile": "main",
      "sourceRoot": "apps/embedding/src",
      "compilerOptions": {
        "tsConfigPath": "apps/embedding/tsconfig.app.json"
      }
    }
  }
}